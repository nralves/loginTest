{"users":
				[	
					<% @users.each do |user| %>
					{
						
							"id":"<%= user.id %>",
							"title":"<%= user.title %>",
							"name":"<%= user.name %>",
							"organization":"<%= user.organization %>",
							"photo":"<%= user.photo_url(:thumb).to_s %>"	
					},
					<% end %>
					
				]
}